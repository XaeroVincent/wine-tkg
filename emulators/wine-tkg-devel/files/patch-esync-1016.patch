+static void test_condvars_base(RTL_CONDITION_VARIABLE *cv)
+{
+    HANDLE hp, hc;
+    DWORD dummy;
+    BOOL ret;
+
+    if (!pInitializeConditionVariable) {
+        /* function is not yet in XP, only in newer Windows */
+        win_skip("no condition variable support.\n");
