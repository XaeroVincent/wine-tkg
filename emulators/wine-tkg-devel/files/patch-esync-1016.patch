+        if (!entry) continue;
+        fprintf( stderr, "  %04x: ref=%d hash=%d \"",
+                 entry->atom, entry->count, entry->hash );
+        dump_strW( entry->str, entry->len, stderr, "\"\"");
+        fprintf( stderr, "\"\n" );
+    }
